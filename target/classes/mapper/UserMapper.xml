<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.lzy.dao.UserDao">

<!--   查询所有用户信息 -->
    <select id="queryUserAll" resultType="cn.lzy.entity.User">
        select
            id,
            name,
            pass,
            nikName,
            salt
        from user
    </select>




</mapper>